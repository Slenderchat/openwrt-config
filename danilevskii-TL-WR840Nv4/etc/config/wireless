
config wifi-device '2g'
	option type 'mac80211'
	option channel 'auto'
	option hwmode '11g'
	option path 'platform/10300000.wmac'
	option htmode 'HT20'
	option require_mode 'n'
	option country 'RU'
	option legacy_rates '0'

config wifi-iface 'ap'
	option device '2g'
	option ifname 'ap_2g'
	option network 'lan'
	option mode 'ap'
	option ssid 'Irina'
	option encryption 'psk2+ccmp'
	option key '12051964'

