
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'duckdns'
	option enabled '1'
	option domain 'danilevskii.duckdns.org'
	option username 'danilevskii'
	option password 'a66451c0-37cd-4257-889f-692b8c6766a6'
	option ip_source 'network'
	option ip_network 'wan'
	option force_interval '72'
	option force_unit 'hours'
	option check_interval '10'
	option check_unit 'minutes'
	option update_url 'http://www.duckdns.org/update?domains=[USERNAME]&token=[PASSWORD]&ip=[IP]'
	option lookup_host 'danilevskii.duckdns.org'

config service 'tunnelbroker'
	option enabled '1'
	option domain '546539'
	option username 'Slenderchat'
	option password 'iG-YT3PEEb9hMvM-'
	option interface 'wan'
	option ip_source 'network'
	option ip_network 'wan'
	option force_interval '72'
	option force_unit 'hours'
	option check_interval '10'
	option check_unit 'minutes'
	option update_url 'http://ipv4.tunnelbroker.net/nic/update?username=[USERNAME]&password=[PASSWORD]&hostname=[DOMAIN]&myip=[IP]'
	option lookup_host 'danilevskii.duckdns.org'

