config wifi-device 2G
	option type mac80211
	option channel 6
	option hwmode 11g
	option path platform/10300000.wmac
	option htmode HT20
	option legacy_rates 0
	option country US

config wifi-iface ap_2G
	option device 2G
	option ifname ap_2G
	option network lan
	option mode ap
	option ssid Irina
	option encryption psk2+ccmp
	option key 12051964
	option macaddr 7c:8b:ca:b4:4c:f4

config wifi-iface mesh
	option device 2G
	option ifname mesh_2G
	option network wmesh
	option mode mesh
	option mesh_id Irina
	option mesh_fwding 0
	option encryption sae
	option key 12051964
	option hidden 1
	option macaddr 7c:8b:ca:b4:4c:f5
