package openvpn

config openvpn danilevskii
	option enabled 1
	option client 1
	option dev tun0
	option proto udp
	option remote '91.205.165.159 1194'
	option resolv_retry infinite
	option nobind 1
	option persist_key 1
	option persist_tun 1
	option ca /etc/openvpn/pki/CA.crt
	option cert /etc/openvpn/pki/Danilevskii.crt
	option key /etc/openvpn/pki/Danilevskii.key
	option tls_crypt /etc/openvpn/pki/TLS.key
	option fast_io 1
	option remote_cert_tls server
	option auth SHA224
	option auth_nocache 1
	option ncp_disable 1
	option cipher AES-128-GCM
	option compress lzo
	option verb 3
