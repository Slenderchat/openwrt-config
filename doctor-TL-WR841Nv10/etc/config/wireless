config wifi-device '2g'
	option channel 'auto'
	option country 'RU'
	option htmode 'HT20'
	option hwmode '11g'
	option path 'platform/ahb/18100000.wmac'
	option type 'mac80211'

config wifi-iface 'ap_2g'
	option device '2g'
	option encryption 'psk2+ccmp'
	option ifname 'ap_2g'
	option key 'doctor101'
	option mode 'ap'
	option network 'lan'
	option ssid 'Doctor'
