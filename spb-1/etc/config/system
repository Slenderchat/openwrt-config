
config system
	option hostname 'Spb-1'
	option timezone 'MSK-3'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config led 'led_lan'
	option name 'lan'
	option sysfs 'archer-c50-v1:green:lan'
	option trigger 'switch0'
	option port_mask '0x1e'

config led 'led_wan'
	option name 'wan'
	option sysfs 'archer-c50-v1:green:wan'
	option trigger 'switch0'
	option port_mask '0x01'

config led 'led_wlan2g'
	option name 'wlan2g'
	option sysfs 'archer-c50-v1:green:wlan2g'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'wlan1'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'archer-c50-v1:green:wlan5g'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'wlan0'

