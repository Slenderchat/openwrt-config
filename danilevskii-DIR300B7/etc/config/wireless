
config wifi-device '2g'
	option type 'mac80211'
	option channel 'auto'
	option hwmode '11g'
	option path 'platform/10180000.wmac'
	option htmode 'HT20'
	option require_mode 'n'
	option country 'RU'
	option legacy_rates '0'

config wifi-iface 'ap'
	option device '2g'
	option ifname 'ap_2g'
	option network 'lan'
	option mode 'ap'
	option ssid 'Irina'
	option encryption 'psk2+ccmp'
	option key '12051964'

config wifi-iface 'sta'
	option device '2g'
	option ifname 'sta_2g'
	option network 'wwan'
	option mode 'sta'
	option ssid 'Irina'
	option bssid '74:da:88:48:b7:f2'
	option encryption 'psk2+ccmp'
	option key '12051964'

