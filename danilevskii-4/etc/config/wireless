config wifi-device 2G
	option type mac80211
	option channel auto
	option hwmode 11g
	option path platform/qca953x_wmac
	option htmode HT20
	option legacy_rates 0
	option country RU

config wifi-iface ap_2G
	option device 2G
	option ifname ap_2G
	option network lan
	option mode ap
	option ssid Irina
	option encryption psk2+ccmp
	option key 12051964
	option macaddr 30:b5:c2:a1:55:36
