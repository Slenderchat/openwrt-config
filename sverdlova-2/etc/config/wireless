config wifi-device 2G
	option type mac80211
	option channel 6
	option hwmode 11g
	option path platform/10180000.wmac
	option htmode HT20
	option legacy_rates 0
	option country RU

config wifi-iface mesh
	option device 2G
	option ifname mesh_2G
	option network wmesh
	option mode mesh
	option mesh_fwding 0
	option mesh_id Russia
	option hidden 1
	option macaddr cc:b2:55:a0:39:75
