config system
	option hostname 'Spb'
	option log_size '64'
	option timezone 'MSK-3'
	option ttylogin '0'
	option urandom_seed '0'

config timeserver 'ntp'
	list server '0.ru.pool.ntp.org'
	list server '1.ru.pool.ntp.org'
	list server '2.ru.pool.ntp.org'
	list server '3.ru.pool.ntp.org'
	option enable_server '1'
	option enabled '1'

config led 'led_lan'
	option name 'lan'
	option port_mask '0x1e'
	option sysfs 'c50-v4:green:lan'
	option trigger 'switch0'

config led 'led_wan'
	option name 'wan'
	option port_mask '0x01'
	option sysfs 'c50-v4:green:wan'
	option trigger 'switch0'

config led 'led_wlan2g'
	option name 'wlan2g'
	option sysfs 'c50-v4:green:wlan2g'
	option trigger 'phy0tpt'

config led 'led_wlan5g'
	option name 'wlan5g'
	option sysfs 'c50-v4:green:wlan5g'
	option trigger 'phy1tpt'
